          <JSONPostProcessor guiclass="JSONPostProcessorGui" testclass="JSONPostProcessor" testname="JWT 토큰 추출" enabled="true">
            <stringProp name="JSONPostProcessor.referenceNames">jwt_token</stringProp>
            <stringProp name="JSONPostProcessor.jsonPathExpressions">$.data.accessToken</stringProp>
            <stringProp name="JSONPostProcessor.match_numbers">0</stringProp>
            <stringProp name="JSONPostProcessor.defaultValues">eyJhbGciOiJIUzM4NCJ9.eyJzdWIiOiJjNTg1YzI5Mi0yZTAxLTQzOWUtYWM2Zi02MzE0ZTM4NjQ2NjkiLCJlbWFpbCI6ImRwcDNjMTRjQGhvdG1haWwuY29tIiwiaWF0IjoxNzU4NDc3OTk4LCJleHAiOjE3NTg0ODE1OTh9.-sQA7X0YH5MKdl4NSf14bQexRa5GguE7gDqLsu13iJZXyI0ULJaQy3m7-ElquooE</stringProp>
          </JSONPostProcessor>
          <hashTree/>
          <ResponseAssertion guiclass="AssertionGui" testclass="ResponseAssertion" testname="로그인 성공 확인" enabled="true">
            <collectionProp name="Asserion.test_strings">
              <stringProp name="49586">"success":true</stringProp>
            </collectionProp>
            <stringProp name="Assertion.custom_message"></stringProp>
            <stringProp name="Assertion.test_field">Assertion.response_data</stringProp>
            <boolProp name="Assertion.assume_success">false</boolProp>
            <intProp name="Assertion.test_type">2</intProp>
          </ResponseAssertion>
          <hashTree/>
        </hashTree>

        <!-- WebSocket 연결 설정 -->
        <ConfigTestElement guiclass="HttpDefaultsGui" testclass="ConfigTestElement" testname="WebSocket 연결 설정" enabled="true">
          <stringProp name="HTTPSampler.domain">${BASE_URL}</stringProp>
          <stringProp name="HTTPSampler.port">${SERVER_PORT}</stringProp>
          <stringProp name="HTTPSampler.protocol">${WS_PROTOCOL}</stringProp>
          <stringProp name="HTTPSampler.path">/ws/chat</stringProp>
        </ConfigTestElement>
        <hashTree/>

        <!-- WebSocket 채팅방 입장 -->
        <net.xeustechnologies.jmeter.samplers.WebSocketSampler guiclass="net.xeustechnologies.jmeter.samplers.WebSocketSamplerGui" testclass="net.xeustechnologies.jmeter.samplers.WebSocketSampler" testname="WebSocket 채팅방 입장" enabled="true">
          <stringProp name="WebSocketSampler.responsePattern"></stringProp>
          <stringProp name="WebSocketSampler.requestData">{
  "type": "ENTER",
  "roomId": "${CHAT_ROOM_ID}",
  "sender": "${name}",
  "message": "채팅방에 입장했습니다"
}</stringProp>
          <stringProp name="WebSocketSampler.connectTimeOut">5000</stringProp>
          <stringProp name="WebSocketSampler.responseTimeOut">10000</stringProp>
          <stringProp name="WebSocketSampler.proxyHost"></stringProp>
          <stringProp name="WebSocketSampler.proxyPort"></stringProp>
          <stringProp name="WebSocketSampler.proxyUserName"></stringProp>
          <stringProp name="WebSocketSampler.proxyPassword"></stringProp>
          <boolProp name="WebSocketSampler.readResponse">true</boolProp>
          <stringProp name="WebSocketSampler.implementation">native</stringProp>
          <stringProp name="WebSocketSampler.server">ws://${BASE_URL}:${SERVER_PORT}/ws/chat</stringProp>
          <stringProp name="WebSocketSampler.port">${SERVER_PORT}</stringProp>
          <stringProp name="WebSocketSampler.path">/ws/chat</stringProp>
          <stringProp name="WebSocketSampler.protocol">${WS_PROTOCOL}</stringProp>
          <stringProp name="WebSocketSampler.connectionId">chat_connection_${__threadNum}</stringProp>
          <stringProp name="WebSocketSampler.attributeName"></stringProp>
          <stringProp name="WebSocketSampler.attributeValue"></stringProp>
        </net.xeustechnologies.jmeter.samplers.WebSocketSampler>
        <hashTree>
          <HeaderManager guiclass="HeaderPanel" testclass="HeaderManager" testname="WebSocket 인증 헤더" enabled="true">
            <collectionProp name="HeaderManager.headers">
              <elementProp name="Authorization" elementType="Header">
                <stringProp name="Header.name">Authorization</stringProp>
                <stringProp name="Header.value">Bearer ${jwt_token}</stringProp>
              </elementProp>
              <elementProp name="Sec-WebSocket-Protocol" elementType="Header">
                <stringProp name="Header.name">Sec-WebSocket-Protocol</stringProp>
                <stringProp name="Header.value">authorization</stringProp>
              </elementProp>
            </collectionProp>
          </HeaderManager>
          <hashTree/>
          <ResponseAssertion guiclass="AssertionGui" testclass="ResponseAssertion" testname="채팅방 입장 확인" enabled="true">
